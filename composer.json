{
    "name": "bonusone/doctrine-reverse-engineering",
    "description": "Doctrine entity and repository generator that reverse engineers an existing database for Symfony 6/7 projects.",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "symfony",
        "doctrine",
        "reverse engineering",
        "entity generator"
    ],
    "homepage": "https://liwocha.pl",
    "authors": [
        {
            "name": "Pawel Liwocha",
            "email": "pawel@liwocha.pl"
        }
    ],
    "support": {
        "issues": "https://github.com/bonusone/doctrine-reverse-engineering/issues",
        "source": "https://github.com/bonusone/doctrine-reverse-engineering"
    },
    "require": {
        "php": "^8.2",
        "doctrine/dbal": "^3.8",
        "doctrine/persistence": "^3.2",
        "symfony/config": "^6.4 || ^7.0",
        "symfony/console": "^6.4 || ^7.0",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/filesystem": "^6.4 || ^7.0",
        "symfony/framework-bundle": "^6.4 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "symfony/var-dumper": "^6.4 || ^7.0",
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpstan/phpstan": "^1.11",
        "rector/rector": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DoctrineReverseEngineering\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DoctrineReverseEngineering\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/doctrine-reverse-engineering"
    ],
    "extra": {
        "symfony": {
            "bundle": {
                "DoctrineReverseEngineering\\DoctrineReverseEngineeringBundle": [
                    "all"
                ]
            }
        }
    },
    "suggest": {
        "doctrine/orm": "Provides full Doctrine ORM entity and repository support.",
        "doctrine/doctrine-bundle": "Exposes the ManagerRegistry service inside Symfony applications."
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "prefer-stable": true
}
